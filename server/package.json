{
	"name": "ibm-q-studio",
	"description": "VSCode extension to develop using IBM Q suite",
	"author": "IBM Research",
	"license": "Apache-2.0",
	"licenses": [
		{
		"type": "Apache-2.0",
		"url": "http://www.apache.org/licenses/LICENSE-2.0"
		}
	],
	"publisher": "IBM Research",
	"version": "0.0.1",
	"engines": {
		"node": "*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.ibm.com/IBMQuantum/qiskit-studio"
	},
	"dependencies": {
		"vscode-languageserver": "^3.5.0",
		"antlr4ts": "^0.4.1-alpha.0",
		"antlr4-c3": "^1.1.7",
		"typescript": "^2.7.2"
	},
	"devDependencies": {
		"chai": "^4.1.1",
		"@types/chai": "^4.1.1",
		"mocha": "^3.5.0",
		"antlr4ts-cli": "^0.4.0-alpha.4",
		"ts-mockito": "^2.3.0"
	},
	"scripts": {
		"installServer": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
		"compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -w -p .",
		"compile:parser": "cd src/qasm/antlr && antlr4ts -visitor QasmLexer.g4 QasmParser.g4 && cd ../../..",
		"exportQuantumLibraries": "cp -R src/qasm/libs out/src/qasm",
		"pretest": "npm run compile:parser && tsc test/*.test.ts --outDir out --module commonjs --target ES6 --experimentalDecorators && npm run exportQuantumLibraries ",
		"test": "mocha out/test --recursive",
		"clean": "rm -rf  out node_modules"
	}
}
