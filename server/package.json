{
	"name": "qasm-language-support",
	"description": "QASM language support server",
	"author": "Isma Development Team",
	"license": "MIT",
	"version": "0.0.1",
	"engines": {
		"node": "*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.ibm.com/IBMQuantum/qiskit-studio"
	},
	"dependencies": {
		"vscode-languageserver": "^3.5.0",
		"antlr4ts": "^0.4.1-alpha.0",
		"antlr4-c3": "^1.1.7",
		"typescript": "^2.7.2"
	},
	"devDependencies": {
		"chai": "^4.1.1",
		"@types/chai": "^4.1.1",
		"mocha": "^3.5.0",
		"antlr4ts-cli": "^0.4.0-alpha.4"
	},
	"scripts": {
		"installServer": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
		"compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -w -p .",
		"antlr4ts": "antlr4ts -visitor src/qasm/antlr/Qasm.g4",
		"pretest": "antlr4ts -visitor src/qasm/antlr/Qasm.g4 && tsc test/*.test.ts --module commonjs --target ES6 --experimentalDecorators",
		"test": "mocha"
	}
}
