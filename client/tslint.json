{
	"rules": {
		"file-header": [
			true,
			"Copyright (c) \\d{4}, IBM"
		],
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-unnecessary-class": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true
	},
	"defaultSeverity": "warning",
	"linterOptions": {
		"exclude": [
		]
	}
}